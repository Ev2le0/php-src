			||	}
			|.else
			|	push 8
			||	if (!(op1_info & MAY_BE_REF)) {
			|		LOAD_ZVAL_ADDR FCARG1a, opline->op1_type, opline->op1
			||	}
			|	push FCARG1a
			|.endif
			|	EXT_CALL _array_init, r0
			|.if not X64
			|	add r4, 16
			|.endif
			if (op1_info & MAY_BE_REF) {
				|	mov FCARG1a, [r4] // restore
				|	GET_Z_PTR FCARG1a, FCARG1a
