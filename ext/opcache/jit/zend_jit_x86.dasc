				case ZEND_ASSIGN_SUB:
					|	EXT_CALL zend_jit_assign_dim_sub_helper, r0
					break;
				case ZEND_ASSIGN_MUL:
					|	EXT_CALL zend_jit_assign_dim_mul_helper, r0
					break;
				case ZEND_ASSIGN_DIV:
					|	EXT_CALL zend_jit_assign_dim_div_helper, r0
					break;
