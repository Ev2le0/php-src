	}
	|	EXT_CALL opline->handler, r0
	if (may_throw) {
		zend_jit_check_exception(Dst);
	}
	last_valid_opline++;
	if ((opline+1)->opcode == ZEND_OP_DATA) {
		last_valid_opline++;
	}

	return 1;
}

